{
    "microservices": [
        {
            "name": "e71-spring-boot-ms",
            "stack": "spring-boot",
            "healthCheckPath": "/api/health",
            "roles": ["role1"],
            "env": [
                { 
                    "name": "SPRING_REDIS_HOST",
                    "value": "redis"
                },{ 
                    "name": "REDIS_ACTIVE",
                    "value": "true"
                },{ 
                    "name": "REDIS_ADDRESS",
                    "value": "redis://redis:6379"
                },{ 
                    "name": "SLAVE_INSTANCE_ID",
                    "value": "solr-client"
                },{ 
                    "name": "ENTANDO_RB_CONFIG_ON_FILE",
                    "value": "false"
                },{ 
                    "name": "ENTANDO_RB_CONSUMER_ENTANDO7_CORE_LIB_ACTIVE",
                    "value": "true"
                },{ 
                    "name": "ENTANDO_RB_CONSUMER_ENTANDO7_PORTAL_LIB_ACTIVE",
                    "value": "true"
                },{ 
                    "name": "ENTANDO_RB_CONSUMER_ENTANDO7_PLUGIN_CMS_LIB_ACTIVE",
                    "value": "true"
                },{ 
                    "name": "ENTANDO_RB_CONSUMER_ENTANDO7_CORE_LIB_DEFAULT_GRPC_HOST_PORT",
                    "value": "quickstart-server-grpc-service:8088"
                },{ 
                    "name": "ENTANDO_RB_CONSUMER_ENTANDO7_PORTAL_LIB_DEFAULT_GRPC_HOST_PORT",
                    "value": "quickstart-server-grpc-service:8088"
                },{ 
                    "name": "ENTANDO_RB_CONSUMER_ENTANDO7_PLUGIN_CMS_LIB_DEFAULT_GRPC_HOST_PORT",
                    "value": "quickstart-server-grpc-service:8088"
                }
            ]
        }
    ],
    "microfrontends": [
        {
            "name": "simple-mfe",
            "customElement": "simple-mfe",
            "stack": "react",
            "type": "widget",
            "group": "free",
            "publicFolder": "public",
            "titles": {
                "en": "E71 Simple MFE",
                "it": "E71 Simple MFE"
            },
            "apiClaims": [
                {
                    "name": "be-api",
                    "type": "internal",
                    "serviceName": "e71-spring-boot-ms"
                }
            ],
            "configMfe": "simple-mfe-config",
            "params": [
                {
                    "name": "buttonlabel",
                    "description": "my new button label"
                }
            ]
        },
        {
            "name": "simple-mfe-config",
            "customElement": "simple-mfe-config",
            "stack": "react",
            "type": "widget-config",
            "group": "free",
            "publicFolder": "public"
        }
    ],
    "svc": [
        "keycloak"
    ],
    "name": "e71-spring-boot",
    "version": "0.0.7",
    "description": "e71-react-widget description",
    "type": "bundle"
}
